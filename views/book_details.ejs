<h1>Details</h1>

<img
  src="<%= `http://covers.openlibrary.org/b/isbn/${isbn}-M.jpg` %>"
  class="cover-image"
/>

<p><%= result.title %></p>
<p><%= author_name %></p>

<% if (typeof result.description !== 'undefined') { %>
<p><%= result.description.value %></p>
<% } else { %>
<p>No description available.</p>
<% } %>

<script src="/javascripts/cover_image.js"></script>
